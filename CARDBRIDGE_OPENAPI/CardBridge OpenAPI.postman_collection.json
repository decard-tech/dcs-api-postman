					"// Pre-request script to generate HMAC-SHA256 signature in Postman",
					"const CryptoJS = require('crypto-js');",
					"",
					"// Define your API secret here",
					"const apiSecret = pm.environment.get('secret-key');",
					"const apiKey = pm.environment.get('api-key');",
					"",
					"",
					"// Function to generate HMAC-SHA256 signature",
					"function getSignature(data, secret) {",
					"    try {",
					"        return CryptoJS.HmacSHA256(data, secret).toString(CryptoJS.enc.Hex);",
					"    } catch (error) {",
					"        throw new Error('Failed to calculate hmac-sha256: ' + error.message);",
					"    }",
					"}",
					"",
					"// Get the request URL and query parameters (if GET request)",
					"const queryParams = pm.request.url.query ? pm.request.url.query.toString() : '';",
					"const requestBody = pm.request.body ? pm.request.body.raw : '';",
					"const nonce = Math.floor(Math.random() * (99999 - 10000 + 1)) + 10000;",
					"",
					"// Combine request URL, query parameters, request body (for POST), and current timestamp",
					"const timestamp = Date.now().toString()",
					"const dataToSign = apiKey + timestamp + nonce + queryParams + requestBody ;",
					"",
					"// Generate the signature",
					"const signature = getSignature(dataToSign, apiSecret);",
					"",
					"// Set the generated signature and timestamp as environment variables",
					"pm.environment.set('X-SIGNATURE', signature);",
					"pm.environment.set('X-TIMESTAMP', timestamp);",
					"pm.environment.set('X-DAPI-NONCE', nonce);",
					"",
					"// Log the signature and timestamp for debugging purposes",
					"console.log('dataToSign:', dataToSign);",
					"console.log('Signature:', signature);",
					"console.log('Timestamp:', timestamp);",
					"console.log('nonce:', nonce);",
					"",
